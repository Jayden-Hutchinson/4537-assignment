<IfModule mod_headers.c>
  Header always set Access-Control-Allow-Origin
  "https://comp4537assignmentclient.netlify.app" Header always set
  Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS" Header always
  set Access-Control-Allow-Headers "Content-Type, Authorization" Header always
  set Access-Control-Allow-Credentials "true"
</IfModule>

# Allow Apache to accept OPTIONS instead of blocking it RewriteEngine On # Don't
block OPTIONS RewriteCond %{REQUEST_METHOD} OPTIONS RewriteRule ^(.*)$ $1 [L] #
Force Apache to pass to Passenger/Node PassengerEnabled on PassengerAppRoot
/home/USERNAME/public_html/COMP4537/assignment/server PassengerAppType node
PassengerStartupFile app.js
